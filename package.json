{
  "name": "dat",
  "version": "7.1.2",
  "description": "versioning and sync tool for data sets",
  "keywords": [
    "data package",
    "dataset",
    "replication",
    "synchronization",
    "versioning"
  ],
  "bin": {
    "dat": "cli.js"
  },
  "scripts": {
    "install-precommit": "echo ./node_modules/.bin/standard > .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "pretty-test": "npm run tape | tap-spec",
    "standard": "standard",
    "tape": "tape tests/*.js",
    "test": "standard && npm run tape"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/maxogden/dat.git"
  },
  "author": "max ogden",
  "license": "BSD-3-Clause",
  "dependencies": {
    "discovery-channel": "^1.2.0",
    "folder-walker": "^1.1.2",
    "hyperdrive": "^1.0.3",
    "level": "^1.3.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "os-homedir": "^1.0.1",
    "pump": "^1.0.1",
    "run-series": "^1.1.4",
    "through2": "^2.0.0",
    "xtend": "^4.0.1"
  },
  "bugs": {
    "url": "https://github.com/maxogden/dat/issues"
  },
  "devDependencies": {
    "mkdirp": "^0.5.0",
    "npm-execspawn": "^1.2.0",
    "sort-stream": "^1.0.1",
    "standard": "^5.0.0",
    "stream-iterate": "^1.1.1",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0",
    "tape-spawn": "^1.3.4"
  },
  "homepage": "https://github.com/maxogden/dat",
  "directories": {
    "test": "tests"
  }
}
